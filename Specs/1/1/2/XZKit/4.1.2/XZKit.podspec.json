        "XZKit/Code/XZKit.h",
        "XZKit/Code/AppLanguage/**/*.{h,m,swift}",
        "XZKit/Code/Foundation/ObjectiveC/{NSBundle,ObjectiveC}.{h,m}",
        "XZKit/Code/Foundation/Swift/{String,Swift,OptionSet,Date}.swift"
      ]
    },
    {
      "name": "AppRedirection",
      "public_header_files": [
        "XZKit/Code/XZKit.h",
        "XZKit/Code/AppRedirection/*.h",
